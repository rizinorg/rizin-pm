RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/radare2"
RZPM_DESC "[pkg] radare2 symstall"

RZPM_INSTALL() {
	if [ "${GLOBAL}" = 1 ]; then
		sys/install.sh
	else
		sys/install.sh ${RZPM_PREFIX}
	fi
}

RZPM_UNINSTALL() {
	if [ "${GLOBAL}" = 1 ]; then
		sys/uninstall.sh
	else
		sys/uninstall.sh "${RZPM_PREFIX}"
	fi
}

RZPM_END
