RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/radare2-regressions"
RZPM_DESC "[r2-tool] radare2-regressions tools (provides r2r and r2-v)"

RZPM_INSTALL() {
	${RZPM_SUDO} ${MAKE} symstall PREFIX="${RZPM_PREFIX}" || exit 1
}

RZPM_UNINSTALL() {
	${RZPM_SUDO} ${MAKE} uninstall PREFIX="${RZPM_PREFIX}"
}

RZPM_END
