RZPM_BEGIN

RZPM_GIT "https://github.com/nowsecure/dirtycow"
RZPM_DESC "[r2-io] Linux's DirtyC0w exploit based IO plugin for r2"

RZPM_INSTALL() {
	${MAKE} || exit 1
	cp -f io_dirtycow.so "${RZPM_PLUGDIR}"/io_dirtycow.${LIBEXT} || exit 1
}

RZPM_UNINSTALL() {
	rm -f "${RZPM_PLUGDIR}/io_dirtycow."*
}

RZPM_END
