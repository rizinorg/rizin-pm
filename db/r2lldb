RZPM_BEGIN

RZPM_GIT "https://github.com/nowsecure/r2lldb"
RZPM_DESC "[r2lldb] lldb as backend for r2"

RZPM_INSTALL() {
	${MAKE} install PYPATH="${RZPM_PYPATH}" PREFIX="${RZPM_PREFIX}" || exit 1
}

RZPM_UNINSTALL() {
	${MAKE} uninstall PYPATH="${RZPM_PYPATH}" PREFIX="${RZPM_PREFIX}" || exit 1
}

RZPM_END
