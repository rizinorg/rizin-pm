RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/radare2-extras"
RZPM_DESC "[r2-arch] ARM64 Vector35 disassembler and analyzer plugins"

RZPM_INSTALL() {
	./configure --prefix="${RZPM_PREFIX}" || exit 1
	${MAKE} arm64v35 || exit 1
	${MAKE} arm64v35-install RZPM_PLUGDIR="${RZPM_PLUGDIR}" || exit 1
}

RZPM_UNINSTALL() {
	rm -f "${RZPM_PLUGDIR}"/*arm_v35*
}

RZPM_END
