RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/r2dec-js" r2dec
RZPM_DESC "[r2-plugin] Experimental Decompiler written in javascript on top of r2"
BINDIR="${RZPM_BINDIR}"

RZPM_INSTALL() {
	CFLAGS="-DRZDEC_HOME=\\\"${RZPM_GITDIR}/r2dec\\\"" ${MAKE} -C p || exit 1
}

RZPM_UNINSTALL() {
	${MAKE} -C p uninstall || exit 1
}

RZPM_END
