RZPM_BEGIN

RZPM_GIT "https://github.com/raimue/avarice"
RZPM_DESC "[syspkg] avarice - gdbserver bridge the avr jtag"

RZPM_INSTALL() {
	./Bootstrap || exit 1
	./configure --prefix="${RZPM_PREFIX}" || exit 1
	${MAKE} -j 4 || exit -1
	${MAKE} install || exit 1
}

RZPM_UNINSTALL() {
	${MAKE} uninstall || exit 1
}

RZPM_END
