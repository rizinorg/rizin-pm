RZPM_BEGIN

RZPM_GIT "https://github.com/danigargu/ida-syms2elf"
RZPM_DESC "[r2-r2pipe-python] Export recognized symbols to the ELF symbol table"

RZPM_INSTALL() {
	mkdir -p "${RZPM_ETCD}"
	echo '$syms2elf=#!pipe python '"'`pwd`'"'/syms2elf.py' > "${RZPM_ETCD}/syms2elf"
}

RZPM_UNINSTALL() {
	rm -f "${RZPM_ETCD}"/syms2elf
}

RZPM_END
