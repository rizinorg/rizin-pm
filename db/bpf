RZPM_BEGIN

RZPM_GIT "https://github.com/rizinorg/rizin-extras"
RZPM_DESC "[rz-arch] BPF disassembler, analyzer, emulator"

RZPM_INSTALL() {
	cd bpf || exit 1
	${MAKE} clean
	${MAKE} || exit 1
	cp *.${LIBEXT} "${RZPM_PLUGDIR}" || exit 1
}

RzPM_UNINSTALL() {
	rm -f ${RZPM_PLUGDIR}/*bpf*
}

RZPM_END

