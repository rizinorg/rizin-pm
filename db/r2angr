RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/radare2-extras"
RZPM_DESC "[r2-angr] ANGR IO plugin for radare2"

RZPM_INSTALL() {
	./configure --prefix="${RZPM_PREFIX}" || exit 1
	${RZPM_SUDO} cp -f r2angr/r2angr-io.py "${RZPM_PLUGDIR}" || exit 1
}

RZPM_UNINSTALL() {
	rm -f "${RZPM_PLUGDIR}/"r2angr-io.py
}

RZPM_END
