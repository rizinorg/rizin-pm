RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/radare2-rlang"
RZPM_DESC "[rlang] C# r2pipe/RLang bin/csharp-cs"

RZPM_INSTALL() {
	./configure --prefix="${RZPM_PREFIX}" || exit 1
	${MAKE} -C csharp || exit 1
	${MAKE} -C csharp install || exit 1
}

RZPM_UNINSTALL() {
	${MAKE} -C csharp uninstall
}

RZPM_END
