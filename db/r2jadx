RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/radare2-extras"
RZPM_DESC "[decompiler] Jadx plugin for radare2"
RZPM_DEPS jadx

RZPM_INSTALL() {
	cd r2jadx || exit 1
	rm -rf node_modules
	${MAKE} || exit 1
	${MAKE} install || exit 1
}

RZPM_UNINSTALL() {
	${MAKE} uninstall || exit 1
}

RZPM_END
