RZPM_BEGIN

RZPM_GIT "https://github.com/mrmacete/rz-ios-kernelcache"
RZPM_DESC "[rz] kernelcache loader for rizin"

RZPM_INSTALL() {
	${MAKE} clean
	${MAKE} || exit 1
        export LIBEXT="`rizin -H LIBEXT`"
	cp -f bin_kernelcache.${LIBEXT} "${RZPM_PLUGDIR}" || exit 1
}

RZPM_UNINSTALL() {
        export LIBEXT="`rizin -H LIBEXT`"
	rm -f "${RZPM_PLUGDIR}/bin_kernelcache.${LIBEXT}" || exit 1
	${MAKE} uninstall
}

RZPM_END
