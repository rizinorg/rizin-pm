RZPM_BEGIN

RZPM_GIT "https://github.com/radareorg/radare2-extras"
RZPM_DESC "[r2-fs] read-only SquashFS read support"

RZPM_INSTALL() {
	./configure --prefix="${RZPM_PREFIX}" || exit 1
	cd libr/fs/squashfs || exit 1
	${MAKE}
}

RZPM_UNINSTALL() {
	cd `r2 -H USER_PLUGINS` && rm -f fs_squash*
}

RZPM_END
